<?xml version="1.0" standalone="no"?>
<!DOCTYPE swatml SYSTEM "http://swat.silverorange.com/swatml1.dtd">
<swatml>
	<widget class="SwatDetailsView" id="details_view">
		<object class="SwatDetailsViewField" id="hour">
			<property name="title" translatable="yes">Credit Hours</property>
			<object class="SwatNumericCellRenderer">
				<property name="value" type="data">hours</property>
			</object>
		</object>
		<object class="SwatDetailsViewField" id="expiry_date">
			<property name="title" translatable="yes">Expiry Date</property>
			<object class="SwatDateCellRenderer">
				<property name="format" type="constant">SwatDate::DF_DATE</property>
				<property name="date" type="data">expiry_date</property>
			</object>
		</object>
	</widget>
</swatml>
